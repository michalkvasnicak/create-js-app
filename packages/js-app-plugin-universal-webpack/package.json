{
  "name": "js-app-plugin-universal-webpack",
  "version": "0.3.1",
  "description": "Webpack client side compiler plugin for Create JS App",
  "license": "MIT",
  "author": "Michal Kvasničák (https://twitter.com/@michalkvasnicak)",
  "repository": "michalkvasnicak/create-js-app",
  "bugs": {
    "url": "https://github.com/michalkvasnicak/create-js-app/issues"
  },
  "engines": {
    "node": ">=6.0"
  },
  "engineStrict": true,
  "main": "build/index.js",
  "files": [
    "build",
    "README.md"
  ],
  "dependencies": {
    "assets-webpack-plugin": "3.4.0",
    "autoprefixer": "6.4.1",
    "babel-core": "6.16.0",
    "babel-eslint": "7.0.0",
    "babel-loader": "6.2.5",
    "babel-preset-js-app": "^0.2.1",
    "case-sensitive-paths-webpack-plugin": "1.1.4",
    "css-loader": "0.25.0",
    "eslint-loader": "1.5.0",
    "extract-text-webpack-plugin": "2.0.0-beta.4",
    "fake-style-loader": "1.0.1",
    "file-loader": "0.9.0",
    "find-cache-dir": "0.1.1",
    "fs-extra": "0.30.0",
    "json-loader": "0.5.4",
    "object-assign": "4.1.0",
    "node-fetch": "1.6.3",
    "postcss-loader": "0.13.0",
    "promise": "7.1.1",
    "react-dev-utils": "0.2.1",
    "source-map-support": "0.4.2",
    "style-loader": "0.13.1",
    "url-loader": "0.5.7",
    "webpack": "2.1.0-beta.25",
    "webpack-dev-server": "2.1.0-beta.8",
    "webpack-node-externals": "1.4.3",
    "whatwg-fetch": "1.0.0"
  },
  "devDependencies": {
    "ava": "^0.16.0",
    "babel-cli": "6.14.0",
    "babel-plugin-transform-flow-strip-types": "6.14.0",
    "babel-plugin-transform-object-rest-spread": "6.8.0",
    "babel-preset-latest-minimal": "1.1.1",
    "babel-register": "6.14.0",
    "rimraf": "2.5.4"
  },
  "optionalDependencies": {
    "fsevents": "^1.0.14"
  },
  "scripts": {
    "build": "rimraf build && babel src -d build --ignore '**/__tests__/**'",
    "prepublish": "npm run build",
    "test": "ava"
  },
  "ava": {
    "files": [
      "src/**/__tests__/*.js"
    ],
    "source": [
      "src/**/*.js"
    ],
    "concurrency": 5,
    "failFast": true,
    "tap": true,
    "powerAssert": false,
    "require": [
      "babel-register"
    ],
    "babel": "inherit"
  },
  "babel": {
    "presets": [
      "latest-minimal"
    ],
    "plugins": [
      "transform-flow-strip-types",
      "transform-object-rest-spread"
    ]
  }
}
